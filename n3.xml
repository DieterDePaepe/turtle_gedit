<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <!-- http://www.w3.org/TeamSubmission/n3/#mimetype -->
  <mime-type type="text/n3">
    <comment>N3 (Notation3) RDF serialization format</comment>
    <glob pattern="*.n3"/>
  </mime-type>
  <!-- http://www.w3.org/TeamSubmission/turtle/#sec-mediaReg -->
  <mime-type type="text/turtle">
    <comment>Turtle RDF serialization format</comment>
    <glob pattern="*.ttl"/>
  </mime-type>
</mime-info>
